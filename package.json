{
  "name": "progem-site-integrado",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "predev": "node scripts/theme-build.mjs",
    "dev": "npm run predev && concurrently -k -n FRONT,BFF \"npm:dev:frontend\" \"npm:dev:bff\"",
    "dev:frontend": "vite",
    "dev:bff": "cross-env NODE_ENV=development PORT=8787 nodemon --signal SIGINT server/index.js",
    "dev:server": "nodemon server/index.js",
    "dev:client": "vite",
    "dev:static": "npm run predev && cross-env VITE_DISABLE_BOOTSTRAP=1 vite",
    "preview": "vite preview",
    "preview:static": "cross-env VITE_DISABLE_BOOTSTRAP=1 vite preview",
    "build": "vite build",
    "prebuild:tenant": "node scripts/theme-build.mjs",
    "build:tenant": "npm run prebuild:tenant && vite build"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.471.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.9"
  },
  "engines": {
    "node": "22.x"
  }
}
