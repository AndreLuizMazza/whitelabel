{
  "version": 2,
  "functions": {
    "api/**.js": { "runtime": "nodejs18.x" }
  },
  "buildCommand": "npm run build:tenant",
  "outputDirectory": "dist",
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
